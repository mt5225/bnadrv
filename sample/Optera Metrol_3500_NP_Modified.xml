        <deviceCommand>
            <!-- login -->
            <guid>0E8DB22F-DEF8-987E-1AFB-7BE8BBCC9BEA</guid>
            <condition test="%accessMode% -EQ- telnet">
                <interaction optional="true" regex="true">
                    <prompt>([Uu]ser.*:|[Ll]ogin.*:)</prompt>
                    <command>%username%</command>
                    <response>[Pp]assword:</response>
                    <error retry="true">([Uu]ser.*:|[Ll]ogin.*:)</error>
                </interaction>
                <interaction regex="true">
                    <prompt>[Pp]assword:</prompt>
                    <command sensitive="true">%password%</command>
                    <pauseSeconds>1</pauseSeconds>
                    <error retry="true">([Pp]assword:|Login incorrect)</error>
                    <error retry="true">([Uu]ser.*:|[Ll]ogin.*:)</error>
                </interaction>
            </condition>
        </deviceCommand>
        <deviceCommand>
            <!-- discover core -->
            <guid>1E91FAD6-FBB7-5D7C-120F-ABD77583A086</guid>
            <condition test="%accessMode% -EQ- telnet">
                <interaction>
                    <prompt>&gt;</prompt>
                    <command>telnet %device.aux_host% 10001</command>
                    <response>&lt;</response>
                </interaction>
                <interaction>
                    <prompt>&lt;</prompt>
                    <command>ACT-USER::%privUsername%:BAK1::%privPassword%;</command>
                    <pauseSeconds>5</pauseSeconds>
                    <response>&lt;</response>
                </interaction>
                <interaction>
                    <prompt>&lt;</prompt>
                    <command>SAV-PROV-SOC:::BACK2::,:CHKALM=N;</command>
                    <pauseSeconds>120</pauseSeconds>
                    <response>SOC Database Save Completed</response>
                </interaction>
                <interaction>
                    <prompt>&lt;</prompt>
                    <command>CANC-USER::%privUsername%:SAV1;;</command>
                    <response>&lt;</response>
				</interaction>
				<interaction>
					<prompt>&lt;</prompt>                
					<command omitEOL="true">\x01D</command>                  
					<response>&gt;</response>
				</interaction>
				<interaction>
					<prompt>&gt;</prompt>
					<command>open %device.host% 23</command>                
					<response>Do you want to send anyway(y/n):</response>                
				</interaction>
				<interaction>
					<prompt>Do you want to send anyway(y/n):</prompt>
					<command>y</command>                
					<response>&gt;</response>                
				</interaction>
			</condition>
            <condition test="%transferMode% -EQ- ftp">
                <interaction>
                    <prompt>&gt;</prompt>
                    <command>ftp %device.aux_host%</command>
                    <pauseSeconds>5</pauseSeconds>
                    <response>220</response>
                    <error>no address associated with name</error>
                    <error>Connection refused</error>
                    <error>timed out</error>
                </interaction>
                <interaction>
                    <prompt>220</prompt>
                    <command>%privUsername%</command>
                    <response>331</response>
                </interaction>
                <interaction>
                    <prompt>331</prompt>
                    <command sensitive="true">%privPassword%</command>
                    <response>ftp&gt;</response>
                    <error>failed</error>
                    <error>incorrect</error>
                </interaction>
                <interaction>
                    <prompt>ftp&gt;</prompt>
                    <command>cd saverest</command>
                    <response>ftp&gt;</response>
                </interaction>
            </condition>
        </deviceCommand>
